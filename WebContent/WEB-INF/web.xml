<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
	<display-name>acme</display-name>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>


	<servlet>
		<description></description>
		<display-name>LoginController</display-name>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>acme.controller.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/LoginController</url-pattern>
	</servlet-mapping>


	<servlet>
		<description></description>
		<display-name>AccessController</display-name>
		<servlet-name>AccessController</servlet-name>
		<servlet-class>acme.controller.AccessController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AccessController</servlet-name>
		<url-pattern>/AccessController</url-pattern>
	</servlet-mapping>


	<servlet>
		<description></description>
		<display-name>LevelController</display-name>
		<servlet-name>LevelController</servlet-name>
		<servlet-class>acme.controller.LevelController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LevelController</servlet-name>
		<url-pattern>/LevelController</url-pattern>
	</servlet-mapping>


	<servlet>
		<description></description>
		<display-name>TableLevelController</display-name>
		<servlet-name>TableLevelController</servlet-name>
		<servlet-class>acme.controller.TableLevelController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TableLevelController</servlet-name>
		<url-pattern>/TableLevelController</url-pattern>
	</servlet-mapping>


	<filter>
		<filter-name>AuthFilter</filter-name>
		<filter-class>acme.util.AuthFilter</filter-class>
		<init-param>
			<param-name>test-param</param-name>
			<param-value>Initialization Parameter</param-value>
		</init-param>
	</filter>

	<filter-mapping>
		<filter-name>AuthFilter</filter-name>
		<url-pattern>/admin/*</url-pattern>
		<url-pattern>/home/*</url-pattern>
	</filter-mapping>

</web-app>